<div class="form-container" style="width: 100%">
    ## registry list template
    <h2>#springMessage('registry.title')</h2>
    <table class="table">
        <caption>#springMessage('registry.subtitle')</caption>
        <thead>
            <tr>
                <th>id</th>
                <th>#springMessage('registry.table.user')</th>
                <th>#springMessage('registry.table.op')</th>
                <th>#springMessage('registry.table.time')</th>
            </tr>
        <tbody>
            #foreach($register in $registry)
            <tr>
                <td>${registry.id}</td>
                <td>${registry.user}</td>
                <td>${registry.op}</td>
                <td>${report.recordTime}</td>
            </tr>        
            #end
        </tbody>
        </thead>
    </table>
</div>